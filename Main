    Private Sub btncheck_Click(ByVal sender As Object, ByVal e As EventArgs) Handles btncheck.Click
        pnlcoffee.Visible = False
        pnlbread.Visible = False
        pnlpizza.Visible = False
        pnllist.Visible = True
        markpiz.Visible = False
        markbread.Visible = False
        marklist.Visible = True
        markcoffee.Visible = False
        panelvoid.Visible = False
        panelhome.Visible = False

        markcombo.Visible = False
        pnlcombo.Visible = False
        txtamounttendered.Focus()

        If DataGridView1.RowCount > 0 Then
            Dim iTotal As Integer = 0

            For index As Integer = 0 To DataGridView1.RowCount - 1
                iTotal += Convert.ToInt32(DataGridView1.Rows(index).Cells(5).Value)
            Next
            ttlprice = iTotal
            txtvat.Text = "Php: " & iTotal * 0.12
            txtdiscount.Text = "0%"
            txtsubtotal.Text = "Php: " & iTotal - (iTotal * 0.12)
            txtpayable.Text = iTotal
        End If
    End Sub


