    Private Sub NumbersOnly(ByVal e As System.Windows.Forms.KeyPressEventArgs)
        If (Asc(e.KeyChar) >= 48 And Asc(e.KeyChar) <= 57 Or Asc(e.KeyChar) = 8 Or Asc(e.KeyChar) = 13) Then
        Else
            e.Handled = True
            MessageBox.Show("Please enter Quantity (Numbers)", "ERROR!")
        End If
    End Sub
    Private Sub Timer1_Tick(ByVal sender As Object, ByVal e As EventArgs) Handles Timer1.Tick
       
        timeanddate.Text = Date.Now.ToString("MMMM dd yyyy")
        time.Text = Date.Now.ToString("hh:mm:ss")

    End Sub
    Private Sub btnpizza_Click(ByVal sender As Object, ByVal e As EventArgs) Handles btnpizza.Click
