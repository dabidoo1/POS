            resibo.AppendText(vbTab & Convert.ToInt32(DataGridView1.Rows(index).Cells(4).Value) & vbTab & Convert.ToString(DataGridView1.Rows(index).Cells(1).Value) & vbTab & vbTab & Convert.ToInt32(DataGridView1.Rows(index).Cells(5).Value) & ".00" + vbNewLine)
