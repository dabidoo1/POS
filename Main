    Private Sub btnpay_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnpay.Click
        If DataGridView1.Rows.Count < 1 Then
            MessageBox.Show("Cell is empty")
            txtamounttendered.Focus()
        Else

            If txtamounttendered.Text <> "" Then
                Dim amount = CDec(txtpayable.Text)
                If amount <= txtamounttendered.Text And txtamounttendered.Text > 0 Then
                    MessageBox.Show("Thank you for using our service!", "Success")
                    txtchange.Text = (txtamounttendered.Text - amount)
                    btnreceipt.Enabled = True
                    btnpay.Enabled = False
                    btnvoid.Enabled = False
                    regular.Enabled = False
                    pwd.Enabled = False
                    seniorcitizen.Enabled = False
                    btnbread.Enabled = False
                    btncoffee.Enabled = False
                    btnpizza.Enabled = False
                    btncheck.Enabled = False

                ElseIf txtamounttendered.Text = 0 Then
                    MessageBox.Show("Zero tendered amount", "Error!")
                    txtamounttendered.Text = ""
                    txtamounttendered.Focus()
                Else
                    MessageBox.Show("Not enough money", "Insuffcient tendered amount")
                    txtamounttendered.Text = ""
                    txtamounttendered.Focus()
                End If
            Else
                MessageBox.Show("Enter tendered amount first", "Error!")
                txtamounttendered.Focus()
            End If
        End If
    End Sub

