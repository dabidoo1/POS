    Private Sub quantcomb1_TextChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles quantcomb1.TextChanged
        If quantcomb1.Text <> "" Then
            Dim quant = CDec(quantcomb1.Text)
            If quant > stckbac.Text Or quant > stckchil.Text Then
                MessageBox.Show("Not enough stock", "Error!")
                quantcomb1.Text = ""
            End If
            itemcost(20) = (quant * 849)
        Else
            quantcomb1.Text = ""
        End If
        If stckbac.Text < 10 And stckbac.Text > 5 Then
            stckbac.ForeColor = Color.DarkOrange
        ElseIf stckbac.Text > 0 And stckbac.Text < 6 Then
            stckbac.ForeColor = Color.Red
        ElseIf stckbac.Text <= 0 Then
            chkbac.Text = "OUT OF STOCK"
            chkbac.Enabled = False
            stckbac.ForeColor = Color.Red
        Else
            stckbac.ForeColor = Color.Lime
        End If
        If stckchil.Text < 10 And stckchil.Text > 5 Then
            stckchil.ForeColor = Color.DarkOrange
        ElseIf stckchil.Text > 0 And stckchil.Text < 6 Then
            stckchil.ForeColor = Color.Red
        ElseIf stckchil.Text <= 0 Then
            chkchil.Text = "OUT OF STOCK"
            chkchil.Enabled = False
            stckchil.ForeColor = Color.Red
        Else
            stckchil.ForeColor = Color.Lime
        End If
    End Sub
